<?xml version="1.0" encoding="UTF-8"?>




<LOGGING xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="logging.xsd">
    <LEVELS>
        <LOGLEVEL>
            <LEVEL>DEBUG</LEVEL>
        </LOGLEVEL>
        <LOGLEVEL>
            <LEVEL>INFO</LEVEL>
        </LOGLEVEL>
        <LOGLEVEL>
            <LEVEL>WARNING</LEVEL>
        </LOGLEVEL>
        <LOGLEVEL>
            <LEVEL>ERROR</LEVEL>
        </LOGLEVEL>
        <LOGLEVEL>
            <LEVEL>FATAL</LEVEL>
        </LOGLEVEL>
    </LEVELS>


    <SUBSYSTEMS>
        <SYSTEM>
            <NAME>FSM</NAME>
            <SHORT_NAME>FSM</SHORT_NAME>
            <TAG>--fsm</TAG>
            <TAG_SHORT>--fsm</TAG_SHORT>
            <DEFAULT>WARNING</DEFAULT>
            <CAN_MODIFY>true</CAN_MODIFY>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </SYSTEM>
        <SYSTEM>
            <NAME>MESSAGE</NAME>
            <SHORT_NAME>MSG</SHORT_NAME>
            <TAG>--message</TAG>
            <TAG_SHORT>--msg</TAG_SHORT>
            <DEFAULT>WARNING</DEFAULT>
            <CAN_MODIFY>true</CAN_MODIFY>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </SYSTEM>
        <SYSTEM>
            <NAME>COM</NAME>
            <SHORT_NAME>COM</SHORT_NAME>
            <TAG>--com</TAG>
            <TAG_SHORT>--com</TAG_SHORT>
            <DEFAULT>WARNING</DEFAULT>
            <CAN_MODIFY>true</CAN_MODIFY>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </SYSTEM>        
        <SYSTEM> 
            <NAME>XML</NAME>
            <SHORT_NAME>XML</SHORT_NAME>
            <TAG>--xml</TAG>
            <TAG_SHORT>--xml</TAG_SHORT>
            <DEFAULT>WARNING</DEFAULT>
            <CAN_MODIFY>false</CAN_MODIFY>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </SYSTEM>


    </SUBSYSTEMS>

</LOGGING>
