<!-- Analog external sensors -->


    <SENSOR_ANALOG_DIRECT>
        <DEVICE_ADDRESS>
            <DEVICE_ID>S1</DEVICE_ID>
            <DEVICE_NAME>Flow line temperature</DEVICE_NAME>
            <IP_ADDRESS>192.168.1.12</IP_ADDRESS>
            <CARD>
                <MANUFACTURER_ID>POWER-IO-01</MANUFACTURER_ID>
                <CARD_NAME>PowerIO</CARD_NAME>
                <CARD_ID>4</CARD_ID>
                <DEVICE_PORT_NO>20</DEVICE_PORT_NO>
            </CARD>
        </DEVICE_ADDRESS>
        <DEVICE_TYPE>SensorAnalogDirect</DEVICE_TYPE>
        <UNIT_CONVERSION>
            <INPUT_TYPE>0-10V</INPUT_TYPE>
            <!-- <OUTPUT_UNIT>V</OUTPUT_UNIT> -->
            <MIN_INPUT>0</MIN_INPUT>
            <MAX_INPUT>10</MAX_INPUT>
            <MIN_OUTPUT>-40</MIN_OUTPUT>
            <MAX_OUTPUT>85</MAX_OUTPUT>
        </UNIT_CONVERSION>
        <THRESHOLDS>
            <ALARM_LOW></ALARM_LOW>
            <WARNING_LOW></WARNING_LOW>
            <WARNING_HIGH></WARNING_HIGH>
            <ALARM_HIGH></ALARM_HIGH>
        </THRESHOLDS>
        <DEVICE_ID_AUX>???</DEVICE_ID_AUX>
    </SENSOR_ANALOG_DIRECT>

