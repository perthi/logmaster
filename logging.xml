<!-- Stepper motors -->
    <MOTOR_STEPPER>
        <DEVICE_ADDRESS>
            <DEVICE_ID>M2</DEVICE_ID>
            <DEVICE_NAME>Open/close motor starboard</DEVICE_NAME>
            <IP_ADDRESS>192.168.1.12</IP_ADDRESS>
            <CARD>
                <MANUFACTURER_ID>X-NUCLEO-IHM03A1</MANUFACTURER_ID>
                <CARD_NAME>Stepper motor driver 10 A</CARD_NAME>
                <CARD_ID>2</CARD_ID>
                <DEVICE_PORT_NO>1</DEVICE_PORT_NO>
            </CARD>
        </DEVICE_ADDRESS>
        <DEVICE_TYPE>MotorStepper</DEVICE_TYPE>
        <STEP_UNIT_CONVERSION>
            <UNIT>angle_deg</UNIT>
            <UNIT_PER_STEP>0.000078</UNIT_PER_STEP>
        </STEP_UNIT_CONVERSION>
        <MOTOR_CONFIG>
            <DIRECTION_POSITIVE>CW</DIRECTION_POSITIVE>
            <ENDSTOP_SOFT_POSITIVE></ENDSTOP_SOFT_POSITIVE>
            <ENDSTOP_SOFT_NEGATIVE></ENDSTOP_SOFT_NEGATIVE>
            <MAX_SPEED>1</MAX_SPEED>
            <SPEED>0.06</SPEED>
            <MAX_ACCELERATION>0.01</MAX_ACCELERATION>
            <ACCELERATION>0.01</ACCELERATION>
            <MAX_CURRENT>10</MAX_CURRENT>
            <CURRENT>9</CURRENT>
            <INACTIVE_TIMEOUT>100</INACTIVE_TIMEOUT>
            <INACTIVE_CURRENT>0.1</INACTIVE_CURRENT>
            <HOME_DEVICE_ID></HOME_DEVICE_ID>
            <HOME_VALUE>0</HOME_VALUE>
            <HOME_DIRECTION></HOME_DIRECTION>
            <ENDSTOP_DEVICE_ID_POSITIVE></ENDSTOP_DEVICE_ID_POSITIVE>
            <ENDSTOP_DEVICE_ID_NEGATIVE></ENDSTOP_DEVICE_ID_NEGATIVE>
        </MOTOR_CONFIG>
    </MOTOR_STEPPER>



