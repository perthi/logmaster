<?xml version="1.0" encoding="UTF-8"?>




<LOGGING xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="logging.xsd">
    <LEVELS>
        <LOGLEVEL>
            <LEVEL>DEBUG</LEVEL>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </LOGLEVEL>
        <LOGLEVEL>
            <LEVEL>INFO</LEVEL>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </LOGLEVEL>

        <LOGLEVEL>
            <LEVEL>WARNING</LEVEL>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </LOGLEVEL>

        <LOGLEVEL>
            <LEVEL>ERROR</LEVEL>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </LOGLEVEL>

        <LOGLEVEL>
            <LEVEL>FATAL</LEVEL>
            <FORCE_OUTPUT>false</FORCE_OUTPUT>
        </LOGLEVEL>

        <LOGLEVEL>
            <LEVEL>FORCE</LEVEL>
            <FORCE_OUTPUT>true</FORCE_OUTPUT>
        </LOGLEVEL>
    </LEVELS>

    <SUBSYSTEMS>

        <SYSTEM>
            <NAME>XML</NAME>
            <SHORT_NAME>XML</SHORT_NAME>
            <TAG>--xml</TAG>
            <DEFAULT>WARNING</DEFAULT>
            <CAN_MODIFY>false</CAN_MODIFY>
        </SYSTEM>

        <SYSTEM>
            <NAME>ANALYSIS</NAME>
            <SHORT_NAME>ANA</SHORT_NAME>
            <TAG>--analysis</TAG>
            <TAG_SHORT>--ana</TAG_SHORT>
            <DEFAULT>WARNING</DEFAULT>
            <CAN_MODIFY>true</CAN_MODIFY>
        </SYSTEM>

        <SYSTEM>
            <NAME>EXCEPTION</NAME>
            <SHORT_NAME>EX</SHORT_NAME>
            <TAG>--exception</TAG>
            <TAG_SHORT>--ex</TAG_SHORT>
            <DEFAULT>ERROR</DEFAULT>
            <CAN_MODIFY>false</CAN_MODIFY>
        </SYSTEM>

    </SUBSYSTEMS>

</LOGGING>
